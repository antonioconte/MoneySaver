<li>
   <i class="fa fa-envelope bg-blue"></i>
   <div class="timeline-item">
      <span class="time"><i class="fa fa-clock-o"></i> <%= item.data %></span>
      <h3 class="timeline-header"><a href="/profile/<%= item.id %>"><%= item.Nome %></a> ha pubblicato</h3>
      <div class="timeline-body">
        <%- item.testo %>

      </div>
      <div class="timeline-footer">
        <% if(canEdit){ %>
      <a href="/post/delete/<%= item.id_post %>" class="btn btn-danger btn-xs"><i class="fa fa-trash-o" aria-hidden="true"></i> Delete</a>
      <% } %>
      </div>
   </div>
</li>
