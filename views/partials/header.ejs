<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <% include head %>
</head>
<body class="hold-transition skin-blue sidebar-mini" ng-app="myApp" ng-controller="mainCtrl">
<span style="display:none" id="ipAddress"><%= ipAddress %></span>
<div class="wrapper">
<header class="main-header">
  <!-- Logo -->
  <a href="/" class="logo">
    <!-- mini logo for sidebar mini 50x50 pixels -->
    <span class="logo-mini"><b>A</b>LT</span>
    <!-- logo for regular state and mobile devices -->
    <span class="logo-lg"><b>Admin</b>LTE</span>
  </a>
  <!-- Header Navbar: style can be found in header.less -->
  <nav class="navbar navbar-static-top">
    <!-- Sidebar toggle button-->
    <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
      <span class="sr-only">Toggle navigation</span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
    </a>

    <div class="navbar-custom-menu">
      <ul class="nav navbar-nav">
        <li class="dropdown user user-menu">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown">
            <img src="/dist/img_profile/<%= AvatarByCookie %>" class="user-image" alt="User Image">
            <span id="id_user" style='display: none'><%= IdByCookie %></span>
            <span id="user_name" class="hidden-xs"><%= NomeByCookie %></span>
          </a>
          <ul class="dropdown-menu">
            <!-- User image -->
            <li class="user-header">

              <p>
                <%= NomeByCookie %> - Web Developer
                <small><%= EmailByCookie %></small>
              </p>
            </li>

            </li>
            <!-- Menu Footer-->
            <li class="user-footer">
              <div class="pull-left">
                <a href="/profile/<%= IdByCookie %>" class="btn btn-default btn-flat">Profile</a>
              </div>
              <div class="pull-right">
                <a href="/logout" ng-click="exit_user(<%= IdByCookie %>)" class="btn btn-default btn-flat">Sign out</a>
              </div>
            </li>
          </ul>
        </li>
        <!-- Control Sidebar Toggle Button -->
        <!-- <li>
          <a href="#" data-toggle="control-sidebar"><i class="fa fa-gears"></i></a>
        </li> -->
      </ul>
    </div>
  </nav>
</header>

<% include sidebar %>
<div style="display: none" ng-controller="CordovaNotificationCtrl">
    <h1>Angular Service For Cordova Notification Plugin</h1>
    <div>
        <p>Device API: {{notificationService.apiVersion()}}</p>
    </div>
</div>
